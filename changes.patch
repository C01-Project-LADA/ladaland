+
+    app.get('/set-session', (req, res) => {
+      req.session.user = sessionUser;
+      res.status(200).json({ message: 'Session set' });
+    });
+
+    app.use('/api', visitedCountriesRoute);
+  });
+
+  it('should return 401 if not authenticated', async () => {
